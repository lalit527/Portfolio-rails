<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title><%= @page_title %></title>
    <%= csrf_meta_tags %>

    <meta name="keywords" content="<%= @seo_keywords %>" />

    <%= stylesheet_link_tag    'portfols', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render "shared/port_nav" %>

    <main role="main">

      <%= render "shared/port_masthead" %>

      <div class="album py-5 bg-light">
        <div class="container">

          <div class="row">
            <%= yield %>
          </div>
        </div>
      </div>
    </main>

    <%= render "shared/port_footer" %>

    <%= soucre_helper "portfolio" %>

  </body>
</html>
